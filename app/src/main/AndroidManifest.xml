<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.androidcrypto.creditcardmanager">

    <application
        android:name="de.androidcrypto.creditcardmanager.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <!-- <activity android:name=".AuthenticateActivity"> -->
        <activity android:name="de.androidcrypto.creditcardmanager.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="de.androidcrypto.creditcardmanager.AddEntryActivity" />
        <activity android:name="de.androidcrypto.creditcardmanager.UpdateEntryActivity" />
        <activity android:name="de.androidcrypto.creditcardmanager.DatabaseImportClipboard" />
        <activity android:name="de.androidcrypto.creditcardmanager.DatabaseImportFile" />
        <activity android:name="de.androidcrypto.creditcardmanager.DatabaseExportFile" />
        <activity android:name="de.androidcrypto.creditcardmanager.MasterkeyExportClipboard" />
        <activity android:name="de.androidcrypto.creditcardmanager.AboutActivity" />
     <!--   <activity android:name=".MainActivity"/> -->
    </application>

</manifest>